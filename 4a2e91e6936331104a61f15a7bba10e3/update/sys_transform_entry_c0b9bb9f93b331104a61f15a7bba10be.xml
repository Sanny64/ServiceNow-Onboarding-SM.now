        <source_field>[Script]</source_field>
        <source_script><![CDATA[answer = (function transformEntry(source) {

	// filter already connected groups
	if(source.u_group_type.toString() == "persona" || source.u_group_type.toString() == "Persona" || source.u_group_type.toString() == "PERSONA")
		return;
	
	//get grouptype 
	var groupType = new GlideRecord('sys_user_group_type');
	groupType.addQuery("name", source.u_group_type.toString());
	groupType.query();
	if(!groupType.hasNext())
		return;
	groupType.next();
	
	var groupTypeId = groupType.sys_id.toString();
		
	return groupTypeId; // return the value to be put into the target field

})(source);]]></source_script>
